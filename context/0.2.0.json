      "@type": "xsd:string"
    },
    
    "credentials": {
      "@id": "cred:verifiableCredential",
      "@type": "@id",
      "@container": "@set"
    },
    
    "supplierMetadata": {
      "@id": "lic:supplierMetadata",
      "@type": "@id"
    },
    
